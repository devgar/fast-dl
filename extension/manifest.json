{
  "manifest_version": 2,

  "name": "fast-dl",
  "description": "Browser extension + local server for fast downloading youtube audio as mp3",
  "version": "0.0.0",

  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "fast-dl"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": [
    "tabs"
  ]
}
